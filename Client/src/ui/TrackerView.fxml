<?import java.net.URL?>
<?import javafx.scene.web.WebView?>

<fx:root fx:id="mainFrame" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="750.0" prefWidth="999.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <children>
                <WebView fx:id="trackerWebView" layoutX="194.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                </WebView>
                <Button id="default-button" fx:id="trackButton" mnemonicParsing="false" onAction="#setTracker" prefHeight="35" prefWidth="80" text="Track" textAlignment="RIGHT" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
            </children>
        </AnchorPane>
    </children>
    <stylesheets>
        <URL value="@../resources/style.css" />
    </stylesheets>
</fx:root>
